{
    "ssl": {
        "enabled": SSL,
        "ssl_key": "/core/cert/wss.pem",
        "ssl_cert": "/core/cert/wss.pem"
    },
    "server": {
        "host": "0.0.0.0",
        "port": 10022,
        "socket": {
            "originHost": false
        }
    },
    "mongodb": {
        "uri": "mongodb://MONGODB_HOST:27017/webitel",
        "collectionPublic": "public",
        "collectionDefault": "default",
        "collectionSystem": "global",
        "collectionAuth": "authToken",
        "collectionCalendar": "calendar"
    },
    "webitelServer": {
        "host": "CONSOLE_HOST",
        "port": "6873",
        "account": "root",
        "secret": "CONSOLE_PASSWORD",
        "reconnect": 5
    },
    "freeSWITCH": {
        "host": "FS_HOST",
        "port": 8021,
        "pwd": "ClueCon",
        "reconnect": 5
    },
    "cdrServer": {
        "host": "CDR_HTTP://CDR_SERVER"
    },
    "application": {
        "loglevel": "LOGLEVEL",
        "logstash": {
            "enabled": LOGSTASH_ENABLE,
            "host": "LOGSTASH_HOST",
            "port": 28777,
            "node_name": "core"
        },
        "auth": {
            "tokenSecretKey": "TOKEN_KEY",
            "expiresDays": 7
        },
        "socketTimeUnauthorized": 5,
        "sleepConnectToWebitel": 1000,
        "freeSWITCHEvents": [
            "CHANNEL_CREATE",
            "CHANNEL_DESTROY",
            "CHANNEL_CALLSTATE",
            "CHANNEL_ANSWER",
            "CHANNEL_HANGUP_COMPLETE",
            "CHANNEL_HANGUP",
            "CHANNEL_HOLD",
            "CHANNEL_UNHOLD",
            "CHANNEL_BRIDGE",
            "CHANNEL_UNBRIDGE",
            "CHANNEL_UUID",
            "DTMF",
            "CHANNEL_DATA"
        ]
    }
}